# .env.example - Environment Variables Template

# Google Gemini API Key (Required for quiz generation and transcription)
# Get your key from: https://aistudio.google.com/app/apikey
GOOGLE_API_KEY=your_gemini_api_key_here

# Optional: Groq API Key (Alternative transcription service)
# Get your key from: https://console.groq.com
GROQ_API_KEY=your_groq_api_key_here

# Base URL (Update for production deployment)
NEXT_PUBLIC_BASE_URL=http://localhost:3000

# Optional: Azure Storage (if using blob storage)
# Optional: Azure Storage (if using blob storage)
AZURE_STORAGE_CONNECTION_STRING=your_connection_string_here

# Quiz Data Sources (Required for Quizzes to Function)
AWS_QUESTIONS_URL=https://example.com/aws-questions.json
AZURE_QUESTIONS_URL=https://example.com/azure-questions.json
SALESFORCE_QUESTIONS_URL=https://example.com/salesforce-questions.json
MONGODB_QUESTIONS_URL=https://example.com/mongodb-questions.json
PCAP_QUESTIONS_URL=https://example.com/pcap-questions.json

